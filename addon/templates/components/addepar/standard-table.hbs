{{#ember-table-2 rows=rows columns=columns as |row rowIndex|}}
  {{#component rowComponent
    row=row
    rowIndex=rowIndex
    columns=columns
    cellCache=cellCache
    numFixedColumns=numFixedColumns
    cellProxyClass=cellProxyClass
    selectedRows=selectedRows

    as |cell|
  }}
    {{yield cell}}
  {{/component}}
{{/ember-table-2}}
