{{#each cells as |cell columnIndex|}}
  {{#component _outerCellComponent
    rowValue=rowValue
    rowIndex=rowIndex
    extraRowData=extraRowData
    column=cell.column
    columnIndex=columnIndex
    numFixedColumns=numFixedColumns
    cell=cell
  }}
    {{yield cell rowValue rowIndex}}
  {{/component}}
{{/each}}
