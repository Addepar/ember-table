{{#each cells as |cell columnIndex|}}
  {{#ember-table-cell
    rowValue=rowValue
    column=cell.column
    columnIndex=columnIndex
    isHovered=isHovered
    isSelected=isSelected
    numFixedColumns=numFixedColumns
    cell=cell
  }}
    {{yield cell columnIndex}}
  {{/ember-table-cell}}
{{/each}}
