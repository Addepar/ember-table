{{#each wrappedRowArray as |rowValue|}}
  {{#-ember-table-private/row-wrapper
    rowValue=rowValue
    columns=columns

    cellMetaCache=cellMetaCache
    columnMetaCache=columnMetaCache
    rowMetaCache=rowMetaCache

    canSelect=canSelect
    rowSelectionMode=rowSelectionMode
    checkboxSelectionMode=checkboxSelectionMode

    as |api|
  }}
    {{#if hasBlock}}
      {{yield (hash
        api=api

        row=(component "ember-tr" api=api)
      )}}
    {{else}}
      {{ember-tr api=api}}
    {{/if}}
  {{/-ember-table-private/row-wrapper}}
{{/each}}
