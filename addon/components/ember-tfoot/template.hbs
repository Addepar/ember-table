{{#each wrappedRows as |row rowIndex|}}
  {{#-ember-table-private/row-wrapper
    rowValue=row.value
    rowParents=row.parents
    rowIsCollapsed=row.isCollapsed

    rowIndex=rowIndex
    selectedRows=selectedRows
    columns=columns

    onSelect=onSelect
    selectMode=selectMode
    selectRow=selectRow
    toggleRowCollapse=toggleRowCollapse

    rowMetaCache=rowMetaCache
    columnMetaCache=columnMetaCache

    as |rowValue rowMeta cells|
  }}
    {{#if hasBlock}}
      {{yield (hash
        selectRow=selectRow
        rowValue=rowValue
        rowMeta=rowMeta
        cells=cells)
      }}
    {{else}}
      {{ember-tr
        api=(hash
          selectRow=selectRow
          rowValue=rowValue
          rowMeta=rowMeta
          cells=cells)
      }}
    {{/if}}
  {{/-ember-table-private/row-wrapper}}
{{/each}}
