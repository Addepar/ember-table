{{#each cells as |cell|}}
  {{#if hasBlock}}
    {{#if api.isHeader}}
      {{yield (hash
        api=cell

        cell=(component "ember-th" api=cell)
      )}}
    {{else}}
      {{yield (hash
        api=cell

        cell=(component "ember-td" api=cell)
      )}}
    {{/if}}
  {{else if api.isHeader}}
    {{ember-th api=cell}}
  {{else}}
    {{ember-td api=cell}}
  {{/if}}
{{/each}}
