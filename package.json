{
  "name": "ember-table",
  "version": "6.0.0-8",
  "description": "An addon to support large data set and a number of features around table.",
  "keywords": [
    "ember-addon"
  ],
  "license": "BSD-3-Clause",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/Addepar/ember-table",
  "scripts": {
    "build": "ember build",
    "docs:deploy": "ember try:one ember-default-docs --- ember deploy production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:files": "ls-lint",
    "lint:js": "eslint .",
    "lint:sass": "npm-run-all lint:sass:*",
    "lint:sass:sass-lint": "sass-lint -c .sass-lint.yml --verbose --no-exit",
    "lint:sass:prettier": "prettier --list-different 'addon/styles/**/*.scss' 'app/styles/**.scss' 'tests/dummy/app/styles/**/*.scss'",
    "release": "release-it",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-proposal-optional-catch-binding": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@html-next/vertical-collection": "^4.0.0",
    "css-element-queries": "^0.4.0",
    "ember-classy-page-object": "^0.8.0",
    "ember-cli-babel": "^7.12.0",
    "ember-cli-htmlbars": "^6.0.0",
    "ember-cli-node-assets": "^0.2.2",
    "ember-cli-version-checker": "^5.1.2",
    "ember-compatibility-helpers": "^1.2.6",
    "ember-raf-scheduler": "^0.3.0",
    "ember-test-selectors": "^6.0.0",
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "@addepar/eslint-config": "^4.0.2",
    "@addepar/prettier-config": "^1.0.0",
    "@addepar/sass-lint-config": "^2.0.1",
    "@addepar/style-toolbox": "~0.8.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.1.2",
    "@ls-lint/ls-lint": "^2.2.3",
    "@tsconfig/ember": "^1.0.1",
    "@types/ember__component": "^4.0.10",
    "babel-eslint": "^10.0.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-a11y-testing": "^5.2.1",
    "ember-auto-import": "^2.4.2",
    "ember-cli": "~3.28.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-faker": "^1.5.0",
    "ember-load-initializers": "^2.0.0",
    "ember-math-helpers": "~2.11.3",
    "ember-qunit": "^5.1.5",
    "ember-radio-button": "^2.0.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "^1.3.1",
    "loader.js": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.19.1",
    "release-it": "^15.5.0",
    "sass": "^1.26.10",
    "sass-lint": "^1.13.1",
    "typescript": "^4.8.4",
    "webpack": "^5.0.0"
  },
  "engines": {
    "node": ">= 14.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "homepage": "https://Addepar.github.io/ember-table",
  "resolutions": {
    "@ember/test-waiters": "^3.0.2",
    "prettier": "1.18.2"
  },
  "typesVersions": {
    "*": {
      "*": [
        "types/*"
      ]
    }
  },
  "volta": {
    "node": "18.20.5",
    "yarn": "1.22.19"
  }
}
