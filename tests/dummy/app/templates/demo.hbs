<div class="demo-container">
  {{#ember-table-2
    rows=rows
    enableColumnResize=true
    enableColumnReorder=true
    hasFixedColumn=true
    columns=columns
    as |cell|
  }}

    <div {{action "onCellClicked" cell}} class={{if cell.wasClicked "red-cell"}}>
      {{cell.value}}
    </div>

  {{/ember-table-2}}
</div>
