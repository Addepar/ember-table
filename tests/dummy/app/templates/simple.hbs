<div class="col-md-10 col-md-offset-2 left-border main-content-container">
  <h1>Ember Table <small>Simple</small></h1>

  <div class="row">
    <div class="col-md-12">
      <div class="example-container">
        <div class="ember-table-example-container">
          {{partial "simple-table/simple-table"}}
        </div>
      </div>
    </div>

    {{!-- TODO(azirbel): Re-enable prettyprint or similar formatter --}}
    <div class="col-md-12 bumper-30">
      <h3>Template</h3>
      <div class="highlight">
        <pre class="prettyprint lang-html">@@{templates/simple-table/simple-table.hbs}</pre>
      </div>
    </div>

    <div class="col-md-12 bumper-30">
      <h3>Controller</h3>
      <div class="highlight">
        <pre class="prettyprint lang-js">@@{controllers/simple.js}</pre>
      </div>
    </div>
  </div>
</div>
