<div class="demo-container">
  {{#ember-table
    rows=rows
    columns=columns
    tableResizeMode="equal_column"
    selectedRows=selectedRows
    onSelect="onSelect"
    as |t|
  }}
    {{#ember-table-row
      row=t
      as |value|
    }}
      {{value}}
    {{/ember-table-row}}
  {{/ember-table}}
</div>
